using System;
namespace a
{
    class b
    {
        public static void Main(String[] args)
        {
            char abc;
            Console.WriteLine("Enter an alphabet");
            abc=Convert.ToChar(Console.ReadLine());
            
            switch(Char.ToLower(abc))
            {
                case'a':
                Console.WriteLine("Vowel");
                break;
                case'e':
                Console.WriteLine("Vowel");
                break;
                case'i':
                Console.WriteLine("Vowel");
                break;
                case'o':
                Console.WriteLine("Vowel");
                break;
                case'u':
                Console.WriteLine("Vowel");
                break;
            }
        }
    }
}
